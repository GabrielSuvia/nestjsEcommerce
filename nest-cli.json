{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "plugins":[{
      "name": "@nestjs/swagger",
      "options":{
        "classValidatorShin":true,
        "introspectComments": true
      }
    }]
  }
}
